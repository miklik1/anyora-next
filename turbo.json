{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [".env"],
  "tasks": {
    "build": {
      "dependsOn": ["^build", "^db:generate"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },
    "db:generate": {
      "cache": false
    },
    "db:push": {
      "cache": false
    },
    "dev": {
      "dependsOn": ["^docker:up", "^db:generate"],
      "persistent": true
    },
    "docker:up": {
      "cache": false,
      "persistent": false
    },
    "docker:down": {
      "cache": false,
      "persistent": false
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "type-check": {},
    "clean": {
      "cache": false
    },
    "test": {
      "dependsOn": ["^test"]
    }
  }
}
